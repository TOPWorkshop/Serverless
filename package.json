{
  "name": "topworkshop-serverless",
  "version": "0.0.1",
  "description": "The repo for a serverless workshop, here in ThinkOpen",
  "scripts": {
    "serverless": "serverless",
    "compile:static": "webpack --config config/webpack/static.js",
    "deploy": "npm run deploy:lambda && npm run deploy:static",
    "deploy:lambda": "serverless deploy",
    "postdeploy:lambda": "serverless invoke --function subscribeToLogs && serverless exportEndpoints && serverless invoke --function telegram-registerEndpoint --path endpoints.json",
    "predeploy:static": "serverless exportEndpoints && npm run compile:static",
    "deploy:static": "serverless client deploy",
    "eslint": "eslint --ext js,vue .",
    "check": "npm run eslint",
    "pretest": "npm run check",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^4.0.0-beta.3",
    "dynamoose": "^0.8.6",
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "aws-sdk": "^2.175.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-vue": "^4.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^4.7.2",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.9",
    "sass-loader": "^6.0.6",
    "serverless": "^1.25.0",
    "serverless-finch": "^1.3.1",
    "serverless-plugin-export-endpoints": "^0.1.2",
    "serverless-webpack": "^3.1.1",
    "style-loader": "^0.19.1",
    "vue-loader": "^13.7.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.3.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-node-externals": "^1.6.0"
  },
